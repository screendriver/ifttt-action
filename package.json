{
    "name": "ifttt-action",
    "version": "1.0.0",
    "description": "GitHub action that triggers an IFTTT webhooks event",
    "main": "target/src/index.js",
    "author": "Christian Rackerseder <git@echooff.de>",
    "license": "MIT",
    "scripts": {
        "compile": "tsc --build",
        "build": "ncc build target/src/index.js --out target/dist/ --minify",
        "lint": "eslint src test --ext '.ts,.tsx'",
        "copy:paste:detector": "jscpd src test",
        "test:unit": "ava",
        "test:unit:coverage": "c8 ava"
    },
    "dependencies": {
        "@actions/core": "1.6.0",
        "got": "11.8.3",
        "tslib": "2.3.1"
    },
    "devDependencies": {
        "@semantic-release/changelog": "6.0.1",
        "@semantic-release/commit-analyzer": "9.0.2",
        "@semantic-release/git": "10.0.1",
        "@semantic-release/github": "8.0.2",
        "@semantic-release/npm": "9.0.1",
        "@semantic-release/release-notes-generator": "10.0.3",
        "@types/got": "9.6.12",
        "@types/sinon": "10.0.11",
        "@typescript-eslint/eslint-plugin": "5.13.0",
        "@typescript-eslint/parser": "5.13.0",
        "@vercel/ncc": "0.33.3",
        "ava": "4.0.1",
        "c8": "7.11.0",
        "eslint": "8.10.0",
        "eslint-config-prettier": "8.4.0",
        "eslint-plugin-ava": "13.2.0",
        "eslint-plugin-prettier": "4.0.0",
        "jscpd": "3.4.5",
        "prettier": "2.5.1",
        "semantic-release": "19.0.2",
        "sinon": "13.0.1",
        "ts-node": "10.6.0",
        "typescript": "4.6.2"
    }
}
