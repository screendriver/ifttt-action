{
  "name": "ifttt-action",
  "version": "1.0.0",
  "description": "GitHub action that triggers an IFTTT webhooks event",
  "main": "target/src/index.js",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc --build",
    "build": "ncc build target/src/index.js --out target/dist/ --minify",
    "lint": "eslint src test --ext '.ts,.tsx'",
    "copy:paste:detector": "jscpd src test",
    "test:unit": "TS_NODE_PROJECT=tsconfig.base.json mocha",
    "test:unit:coverage": "TS_NODE_PROJECT=tsconfig.base.json c8 mocha"
  },
  "dependencies": {
    "@actions/core": "1.6.0",
    "got": "11.8.2",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@types/chai": "4.2.22",
    "@types/got": "9.6.12",
    "@types/mocha": "9.0.0",
    "@types/sinon": "10.0.6",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "@vercel/ncc": "0.31.1",
    "c8": "7.10.0",
    "chai": "4.3.4",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "jscpd": "3.3.26",
    "mocha": "9.1.3",
    "prettier": "2.4.1",
    "semantic-release": "18.0.0",
    "sinon": "12.0.1",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  }
}
