{
  "name": "ifttt-action",
  "version": "1.0.0",
  "description": "GitHub action that triggers an IFTTT webhooks event",
  "main": "target/src/index.js",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc --build",
    "build": "ncc build target/src/index.js --out target/dist/ --minify",
    "lint": "eslint src test --ext '.ts,.tsx'",
    "copy:paste:detector": "jscpd src test",
    "test:unit": "TS_NODE_PROJECT=tsconfig.base.json mocha",
    "test:unit:coverage": "TS_NODE_PROJECT=tsconfig.base.json c8 mocha"
  },
  "dependencies": {
    "@actions/core": "1.3.0",
    "got": "9.6.0",
    "tslib": "2.2.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.2",
    "@types/chai": "4.2.18",
    "@types/got": "9.6.11",
    "@types/mocha": "8.2.2",
    "@types/sinon": "10.0.2",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "@vercel/ncc": "0.28.6",
    "c8": "7.7.2",
    "chai": "4.3.4",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "jscpd": "3.3.26",
    "mocha": "9.0.0",
    "prettier": "2.3.1",
    "semantic-release": "17.4.3",
    "sinon": "11.1.1",
    "ts-node": "10.0.0",
    "typescript": "4.3.2"
  }
}
